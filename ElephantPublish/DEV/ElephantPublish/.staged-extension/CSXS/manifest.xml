<?xml version="1.0" encoding="UTF-8"?>

<ExtensionManifest Version="4.0" ExtensionBundleId="com.buck.ElephantPublish" ExtensionBundleVersion="1.0.4"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ExtensionList>
		<Extension Id="com.buck.ElephantPublish.extension1" Version="1.0.4" />
	</ExtensionList>
	<ExecutionEnvironment>
		<HostList>
			<Host Name="PHSP" Version="[16.0,16.9]" />
			<Host Name="PHXS" Version="[16.0,16.9]" />
		</HostList>
		<LocaleList>
			<Locale Code="All" />
		</LocaleList>
		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="6.0" />

		</RequiredRuntimeList>
	</ExecutionEnvironment>
	<DispatchInfoList>
		<Extension Id="com.buck.ElephantPublish.extension1">
			<DispatchInfo >
				<Resources>
					<MainPath>./index.html</MainPath>
					<CEFCommandLine>
						<Parameter>--enable-nodejs</Parameter>
					</CEFCommandLine>
				</Resources>
				<Lifecycle>
					<AutoVisible>true</AutoVisible>
				</Lifecycle>
				<UI>
					<Type>Panel</Type>
					<Menu>ElephantPublish</Menu>
					<Geometry>
						<Size>
							<Height>400</Height>
							<Width>250</Width>
						</Size>
					</Geometry>
					<Icons>
						<Icon Type="Normal">./images/elephant_icon.png</Icon>
					</Icons>
				</UI>
			</DispatchInfo>
		</Extension>
	</DispatchInfoList>
</ExtensionManifest>